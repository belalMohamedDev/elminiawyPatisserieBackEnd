{
    "version":2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      
       {
        "src": "./uploads/(.*)",
        "dest": "./uploads/$1"
      },
     
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }



